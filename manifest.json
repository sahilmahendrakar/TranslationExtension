{
    "name": "Translation Extension",
    "version": "1.0",
    "description": "Toucan",
    "permissions": ["activeTab", "storage"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["translate.js"]
        }
    ],
    "manifest_version": 2
}